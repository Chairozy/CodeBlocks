script_template = "extends Node2D

var sequences := []

func movement(axis: String, distance: int):
	$AnimatedSprite2D.play(\"walk\")
	if axis == \"x\":
		$AnimatedSprite2D.flip_h = distance < 0
	await create_tween().tween_property(self, \"position:\"+axis, position[axis] + distance, 1.0).finished
	$AnimatedSprite2D.play(\"default\")

"
